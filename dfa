digraph DFA {
	rankdir=LR
	1 [shape=circle]
	1 [fillcolor=green rank=same style=filled]
	1 -> 2 [label=a]
	1 -> 3 [label=b]
	2 [shape=circle]
	2 -> 2 [label=a]
	2 -> 4 [label=b]
	3 [shape=circle]
	3 -> 2 [label=a]
	3 -> 3 [label=b]
	4 [shape=circle]
	4 [rank=same shape=doublecircle]
	4 -> 2 [label=a]
	4 -> 3 [label=b]
}
